apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: /etc/secrets/ca.pem
    server: https://kube.checkrhq-dev.net
  name: kube-aws-development-cluster
contexts:
- context:
    cluster: kube-aws-development-cluster
    namespace: default
    user: kube-aws-development-admin
  name: kube-aws-development-context
users:
- name: kube-aws-development-admin
  user:
    client-certificate: /etc/secrets/admin.pem
    client-key: /etc/secrets/admin-key.pem
current-context: kube-aws-development-context
